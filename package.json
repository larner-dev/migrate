{
  "name": "@larner.dev/migrate",
  "description": "CLI for applying database migrations",
  "version": "1.8.4",
  "author": "Aaron Larner",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"vite build --watch\"",
    "build": "tsc --declaration && vite build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "postversion": "git push && git push --tags",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn build",
    "test": "vitest --run --dir ./src",
    "test-watch": "vitest --dir ./src",
    "test-coverage": "vitest run --dir ./src --coverage"
  },
  "devDependencies": {
    "@types/klaw": "3.0.3",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitest/coverage-c8": "0.33.0",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "nodemon": "3.0.3",
    "rollup-plugin-node-externals": "7.1.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "3.7.2",
    "vitest": "1.2.2"
  },
  "dependencies": {
    "chalk": "5.2.0",
    "chokidar": "3.5.3",
    "commander": "9.5.0",
    "klaw": "4.1.0",
    "pg": "8.8.0",
    "sequelize": "^6.28.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bin": "./dist/cli.js",
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
